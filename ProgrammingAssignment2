makeMatrix<- function(x=numeric()){
  i<-NULL
  set<-function(y){
    x<<-y
    dim(x)<-c(2,2)
    dim(i)<-dim(x)
  }
  get<- function() x
  setinverse <- function(solve) i<<-solve
  getinverse<- function() i
  list(set=set,get=get,setinverse=setinverse,getinverse=getinverse)
}

cacheSolve <- function(x,...){
  i<-x$getinverse()
  if(!is.null(i)){
    message("getting cached data")
    return(i)
  }
  data<-x$get()
  i<-solve(data,...)
  x$setinverse(i)
  i
}
