makeCacheMatrix <- function(x = matrix()) { 
  invm <- NULL 
  set <- function(mn) { 
    x <<- mn 
    invm <<- NULL 
  } 
  
  get <- function() x 
  setInvm <- function(i) invm <<- m 
  getInvm <- function() invm 
  
  list(set=set, 
       get=get, 
       setInv=setInv, 
       getInv=getInv) 
} 
cacheSolve<- function(x, ...){
  invm<-x$getInvm()
  if (!is.null(invm))
  {
    message("get the results")
    return(invm)}
  matrix<-x$get()
  inv<-solve(matrix,...)
  x$setInvm(invm)
}
